    {%- set secret_access_key = src_secret_access_key  -%}
{% endif %}
{% if posix_2_os | bool %}
    {%- set access_key_id = destination_access_key_id  -%}
    {%- set secret_access_key = destination_secret_access_key  -%}
{% endif %}
